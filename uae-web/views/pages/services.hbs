<section role="Energy Optimization">
    <div class="anchor"></div>
    <div class="_energy_optimization">
        <div class="_inner">
            <h2 class="wow fadeIn" data-wow-delay="0.3s">{{data.AnalyticsDashboard}}</h2>
            <p class="mb0 wow fadeIn" data-wow-delay="0.5s">{{data.AnalyticsDashboardDescription}}</p>


            <div class="_energy_section">
                <div class="slider_energy">
                    {{#if AnalyticsDashboard}}
                    {{#each AnalyticsDashboard}}
