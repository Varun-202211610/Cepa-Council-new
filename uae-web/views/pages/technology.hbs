{{> header }}

<!--banner section-->
<section role="banner section">
    <div class="_technology_section"
        style="background : url({{API_URL}}{{data.Banner.url}}) center center/cover no-repeat;"
        data-background="{{API_URL}}{{data.Banner.url}}">
        <div class="overlay"></div>
        <div class="_inner_section">
            <div class="_text_section">
                <h2 class="wow fadeInUp" data-wow-delay="0.5s">{{data.Heading}}</h2>
            </div>
        </div>
    </div>
</section>
<!--banner section-->

<!---about paragraph section-->
<section role="paragraph section">
    <div class="_para_section">
        <div class="_inner_about">
		<p class="wow fadeInUp" data-wow-delay="0.4s" style="visibility: visible; animation-delay: 0.4s; animation-name: fadeInUp;">
Harnessing the power of data management and AI-powered
                analytics, we build a smart digital platform to optimize the performance of buildings at every stage of
                their lifecycle. Our fault detection diagnostics enable predictive maintenance of buildings, while our
                energy optimization platform is also proven to deliver strong energy savings to another level. â€‹</p>
            
        </div>
    </div>
</section>
<!---about paragraph section-->

<!---our benefit section-->
<section role="benefit section">
    <div class="_benefit_section"
        style="background: url({{API_URL}}{{data.BenefitBackground.url}}) center center/cover no-repeat;"
        data-background="{{API_URL}}{{data.BenefitBackground.url}}">
        <div class="overlay"></div>
        <div class="_inner_section">
            <h2 class="wow fadeIn" data-wow-delay="0.5s">Our Benefits</h2>
            <ul>
                {{#if benefits}}
                {{#each benefits}}
                <li class="wow fadeInUp" data-wow-delay="0.2s">
                    <img src="{{../API_URL}}{{Media.url}}">
                    <h2>{{Title}}</h2>
                    <p>{{Description}}</p>
                </li>
                {{/each}}
                {{/if}}
            </ul>
        </div>
    </div>
</section>
<!---our benefit section-->


